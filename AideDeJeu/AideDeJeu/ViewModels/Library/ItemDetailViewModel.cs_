using AideDeJeuLib;

namespace AideDeJeu.ViewModels.Library
{
    public class ItemDetailViewModelOld : BaseViewModel
    {
        Item _Item = null;
        public Item Item
        {
            get { return _Item; }
            set
            {
                SetProperty(ref _Item, value);
            }
        }

        public ItemDetailViewModelOld(Item item = null)
        {
            Title = item?.Name;
            Item = item;
        }
    }




}
